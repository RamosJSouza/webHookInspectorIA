{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "tsx watch --env-file=.env src/server.ts",
    "start": "node dist/server.ts",
    "format": "biome format --write",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "dotenv -e .env -- drizzle-kit migrate",
    "db:migrate:docker": "powershell -Command \"Get-Content src/db/migrations/*.sql | docker exec -i webhooks-db psql -U postgres -d webhooks\"",
    "db:seed": "dotenv -e .env -- tsx src/db/seed.ts",
    "db:seed:docker": "docker-compose --profile seed run --rm seed",
    "db:studio": "dotenv -e .env -- drizzle-kit studio --host 0.0.0.0 --port 4983",
    "db:studio:docker": "docker exec -it webhooks-db psql -U postgres -d webhooks"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.20.0",
  "devDependencies": {
    "@biomejs/biome": "2.3.3",
    "@types/node": "^24.10.0",
    "@types/pg": "^8.15.6",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "drizzle-kit": "^0.31.6",
    "tsx": "^4.20.6",
    "typescript": "~5.9.3"
  },
  "dependencies": {
    "@ai-sdk/google": "^2.0.29",
    "@fastify/cors": "^11.1.0",
    "@fastify/swagger": "^9.5.2",
    "@scalar/fastify-api-reference": "^1.38.1",
    "ai": "^5.0.89",
    "drizzle-orm": "^0.44.7",
    "drizzle-zod": "^0.8.3",
    "fastify": "^5.6.1",
    "fastify-type-provider-zod": "^6.1.0",
    "pg": "^8.16.3",
    "uuidv7": "^1.0.2",
    "zod": "^4.1.12"
  }
}
